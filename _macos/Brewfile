# Usage:
# brew bundle --file=path/to/Brewfile

# Manually install cask-app using: brew install --cask xxx --appdir /Applications
cask_args appdir: '/Applications'

# Taps (third-party repositories)
tap 'homebrew/bundle'
tap 'homebrew/core'

# CLIs
brew 'rectangle'
brew 'tree'

# GUIs
cask 'appcleaner'
cask 'docker'
cask 'spotify'
cask 'visual-studio-code'
